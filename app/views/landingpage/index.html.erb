      <script>
         let c1=document.getElementById("price1");
      let c2=document.getElementById("price2");
       let c3=document.getElementById("price3");
      let c4=document.getElementById("price4");

        function onFunction() {
            console.log('Switch turned ON!');
              c1.style.display="block";
              c2.style.display="none";
              c3.style.display="block";
              c4.style.display="none";
        }
        function offFunction() {
            console.log('Switch turned OFF!');

            c1.style.display="none";
            c2.style.display="block";
            c3.style.display="none";
            c4.style.display="block";

        }

        // Function to handle toggle events
        function toggleSwitch() {
            if (this.checked) {
                onFunction();
            } else {
                offFunction();
            }
        }

        // Attaching the toggleSwitch function to the click event of the switch
        document.getElementById("mySwitch").addEventListener("click", toggleSwitch);
    </script>

